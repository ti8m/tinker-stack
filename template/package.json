{
  "name": "planning-stack-template",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:data": "turbo run build:data",
    "build:msw": "turbo build --filter=@repo/mock-backend",
    "build:prototype": "turbo build --filter=@repo/prototype",
    "dev": "turbo run dev",
    "docs": "turbo run docs",
    "postinstall": "patch-package",
    "lint": "turbo run lint -- --fix",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "rimraf .turbo && turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "concurrently": "latest"
  },
  "devDependencies": {
    "@repo/api": "*",
    "eslint-plugin-import": "latest",
    "patch-package": "latest",
    "prettier": "latest",
    "prettier-plugin-organize-imports": "latest",
    "tsc-alias": "latest",
    "turbo": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "latest",
    "vite-tsconfig-paths": "latest"
  },
  "engines": {
    "node": ">=22"
  },
  "resolutions": {
    "typescript": "latest"
  },
  "type": "module",
  "license": "MIT",
  "workspaces": [
    "api",
    "config/eslint",
    "config/typescript",
    "docs",
    "frontend",
    "mock-data",
    "mock-backend",
    "prototype",
    "ui"
  ],
  "packageManager": "npm@11.4.1"
}