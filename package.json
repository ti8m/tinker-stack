{
  "name": "create-tinker-stack",
  "version": "0.2.8",
  "bin": {
    "create-tinker-stack": "create/index.js"
  },
  "type": "module",
  "files": [
    "template",
    "create"
  ],
  "engines": {
    "node": ">=22.3.0"
  },
  "repository": {
    "url": "https://github.com/ti8m/tinker-stack"
  },
  "keywords": [
    "tinker-stack",
    "react",
    "monorepo",
    "starter",
    "template",
    "typescript",
    "turborepo",
    "prototype",
    "prototyping",
    "msw",
    "faker",
    "frontend",
    "scaffold",
    "create-tinker-stack"
  ],
  "scripts": {
    "prerelease": "find template -name package-lock.json -delete && find template -name tsconfig.tsbuildinfo -delete && find template -name .turbo -type d -prune -exec rm -rf {} +",
    "release": "commit-and-tag-version",
    "test": "npm run test:e2e",
    "test:e2e": "vitest run --no-file-parallelism",
    "upgrade:template": "npx npm-check-updates --interactive --format group -c 2 -w --cwd template",
    "postupgrade:template": "find template -name package-lock.json -delete && find template -name node_modules -type d -prune -exec rm -rf {} +",
    "upgrade:root": "npx npm-check-updates --interactive --format group -c 2"
  },
  "dependencies": {
    "execa": "^9.6.1",
    "inquirer": "^13.0.2"
  },
  "devDependencies": {
    "commit-and-tag-version": "^12.6.1",
    "vitest": "^4.0.15"
  }
}
