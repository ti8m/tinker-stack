{
  "name": "planning-stack-template",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:data": "turbo run build:data",
    "build:msw": "turbo build --filter=@repo/mock-backend",
    "build:prototype": "turbo build --filter=@repo/prototype",
    "clean": "rimraf .turbo && turbo run clean",
    "dev": "turbo run dev",
    "docs": "turbo run docs",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "patch-package",
    "lint": "turbo run lint -- --fix",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "upgrade": "npx npm-check-updates --interactive"
  },
  "devDependencies": {
    "@repo/api": "*",
    "eslint-plugin-import": "^2.32.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsc-alias": "^1.8.16",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=22"
  },
  "type": "module",
  "packageManager": "npm@11.6.1",
  "license": "MIT",
  "workspaces": [
    "api",
    "config/eslint",
    "config/typescript",
    "docs",
    "frontend",
    "mock-data",
    "mock-backend",
    "prototype",
    "ui"
  ],
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
